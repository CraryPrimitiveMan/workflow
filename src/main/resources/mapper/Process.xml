<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.codehub.workflow.mapper.ProcessMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.com.codehub.workflow.entity.dto.Process">
        <id column="PROCESS_ID" property="processId" />
        <result column="PROCESS_NAME" property="processName" />
        <result column="PROCESS_ALIAS" property="processAlias" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        PROCESS_ID, PROCESS_NAME, PROCESS_ALIAS
    </sql>

</mapper>
